<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <!-- Globals -->
    <PropertyGroup Label="Globals">
        <VCProjectVersion>16.0</VCProjectVersion>
        <PlatformToolset>v143</PlatformToolset>
        <WindowsTargetPlatformVersion>10.0</WindowsTargetPlatformVersion>
        <RylogicRoot>$(MSBuildThisFileDirectory)..\..\</RylogicRoot>
        <ScriptPath>$(MSBuildThisFileDirectory)..\..\script\</ScriptPath>
        <PropsPath>$(MSBuildThisFileDirectory)</PropsPath>
        <PackagesPath Condition=" '$(RestorePackagesPath)' == '' ">$(SolutionDir)packages\</PackagesPath>
    </PropertyGroup>

</Project>