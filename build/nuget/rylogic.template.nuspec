<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>LIBRARY_NAME</id>
        <version>VERSION</version>
        <authors>Rylogic</authors>
        <copyright>Copyright (c) Rylogic 2007</copyright>
        <license type="expression">MIT</license>
        <iconUrl>http://www.rylogic.co.nz/images/icon.png</iconUrl>
        <description>
            A toolbox of extension methods, data structures, and helper classes used in tools created by Rylogic.
            Visit: https://www.rylogic.co.nz/
        </description>
        <tags>TAGS</tags>
        <repository type="git" url="https://github.com/psryland/rylogic_code" />
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <releaseNotes>
            Rylogic Library service release
        </releaseNotes>
        <dependencies>
            <!--
            - Global Dependency
            <dependency id="MyLibrary" version="1.2.3" />

            - Platform specific dependency
            <group targetFramework="net9.0">
                <dependency id="MyLibrary" version="1.2.3" />
            </group>
            -->
        </dependencies>
    </metadata>

    <files>
        <!--
        - .NET 9 
        <file src="bin/Release/net9.0/MyLibrary.dll" target="lib/net9.0/" />

        - .NET 9 Windows
        <file src="bin/Release/net9.0-windows/MyLibrary.dll" target="lib/net9.0-windows/" />

        - .NET Framework 4.8.1
        <file src="bin/Release/net481/MyLibrary.dll" target="lib/net481/" />

        - Native projects
        <file src="include/**/*.*" target="build/native/include/" />
        <file src="lib/x64/Release/*.lib" target="build/native/lib/x64/" />
        <file src="lib/x64/Release/*.dll" target="runtimes/win-x64/native/" />

        - Misc files
        <file src="build/props/**/*.*" target="build/props/" />
        <file src="build/targets/**/*.*" target="build/targets/" />
        -->
    </files>
</package>
