{
    "name": "vscode-alignextension",
    "version": "0.0.1",
    "description": "Commands that align code vertically at common characters",
    "license": "MIT",
    "main": ".\\out\\projects\\vscode-alignextension\\src\\extension",
    "displayName": "VSCode.AlignExtension",
    "publisher": "rylogic",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.Align"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.Align",
                "title": "Align"
            }
		],
		"keybindings":[
			{
				"command": "extension.Align",
				"key": "ctrl+shift+]",
				"when": "editorTextFocus"
			}
		]
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    }
}
