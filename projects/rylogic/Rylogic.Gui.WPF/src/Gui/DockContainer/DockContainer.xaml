<DockPanel
	x:Class="Rylogic.Gui.WPF.DockContainer"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:opts="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d" 
	d:DesignHeight="250"
	d:DesignWidth="450"
	Background="{x:Static SystemColors.ControlBrush}"
	>
	<DockPanel.Resources>
		<ResourceDictionary>
			<BitmapImage x:Key ="dock_menu" UriSource="/Rylogic.Gui.WPF;component/res/dock_menu.png" opts:Freeze="True"/>
			<BitmapImage x:Key ="dock_site_top" UriSource="/Rylogic.Gui.WPF;component/res/dock_site_top.png" opts:Freeze="True"/>
			<BitmapImage x:Key ="dock_site_left" UriSource="/Rylogic.Gui.WPF;component/res/dock_site_left.png" opts:Freeze="True"/>
			<BitmapImage x:Key ="dock_site_right" UriSource="/Rylogic.Gui.WPF;component/res/dock_site_right.png" opts:Freeze="True"/>
			<BitmapImage x:Key ="dock_site_bottom" UriSource="/Rylogic.Gui.WPF;component/res/dock_site_bottom.png" opts:Freeze="True"/>
			<BitmapImage x:Key ="dock_site_cross_sm" UriSource="/Rylogic.Gui.WPF;component/res/dock_site_cross_sm.png" opts:Freeze="True"/>
			<BitmapImage x:Key ="dock_site_cross_lg" UriSource="/Rylogic.Gui.WPF;component/res/dock_site_cross_lg.png" opts:Freeze="True"/>
		</ResourceDictionary>
	</DockPanel.Resources>
</DockPanel>