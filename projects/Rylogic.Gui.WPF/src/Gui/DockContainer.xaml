<DockPanel
	x:Class="Rylogic.Gui.WPF.DockContainer"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	xmlns:common="clr-namespace:Rylogic.Common;assembly=Rylogic.Core"
	xmlns:gui2="clr-namespace:Rylogic.Gui.WPF"
	xmlns:local="clr-namespace:Rylogic.Gui.WPF.DockContainerDetail"
	mc:Ignorable="d" 
	d:DesignHeight="250"
	d:DesignWidth="450"
	Background="{x:Static SystemColors.ControlBrush}"
	>
	<DockPanel.Resources>
		<ResourceDictionary>
			<BitmapImage x:Key ="dock_menu" UriSource="../../res/dock_menu.png"/>
			<BitmapImage x:Key ="dock_side_top" UriSource="../../res/dock_side_top.png"/>
			<BitmapImage x:Key ="dock_side_left" UriSource="../../res/dock_side_left.png"/>
			<BitmapImage x:Key ="dock_side_right" UriSource="../../res/dock_side_right.png"/>
			<BitmapImage x:Key ="dock_site_bottom" UriSource="../../res/dock_site_bottom.png"/>
			<BitmapImage x:Key ="dock_site_cross_sm" UriSource="../../res/dock_site_cross_sm.png"/>
			<BitmapImage x:Key ="dock_site_cross_lg" UriSource="../../res/dock_site_cross_lg.png"/>




		</ResourceDictionary>
	</DockPanel.Resources>
</DockPanel>