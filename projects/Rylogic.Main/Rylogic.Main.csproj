<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>NET471</TargetFrameworks>
		<Authors>P Ryland</Authors>
		<Company>Rylogic</Company>
		<Description>A toolbox of extension methods, data structures, and helper classes used in tools created by Rylogic.</Description>
		<Copyright>Copyright (c) Rylogic 2007-2018</Copyright>
		<PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
		<ApplicationIcon>SmilingGekko140x110.ico</ApplicationIcon>
		<RootNamespace>Rylogic</RootNamespace>
		<OutputType>Library</OutputType>
		<StartupObject />
		<AssemblyVersion>1.1.1</AssemblyVersion>
		<FileVersion>1.1.1</FileVersion>
		<Version>1.1.1</Version>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DefineConstants>RELEASE;NET471</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|NET471|AnyCPU'">
		<DefineConstants>TRACE;DEBUG;NET471;PR_UNITTESTS</DefineConstants>
	</PropertyGroup>
	<ItemGroup>
		<None Remove="SmilingGekko140x110.ico" />
	</ItemGroup>
	<ItemGroup>
		<Compile Include="..\..\sdk\scintilla\include\SciLexer.cs" Link="src\scintilla\SciLexer.cs" />
		<Compile Include="..\..\sdk\scintilla\include\Scintilla.cs" Link="src\scintilla\Scintilla.cs" />
	</ItemGroup>
	<ItemGroup>
		<Content Include="Rylogic.Main.targets">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
	<ItemGroup>
		<EmbeddedResource Include="SmilingGekko140x110.ico" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Rylogic.Core\Rylogic.Core.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="System.Windows.Forms" />
		<Reference Include="WindowsBase" />
	</ItemGroup>
	<ItemGroup>
		<Compile Update="Properties\Resources.Designer.cs">
			<DependentUpon>Resources.resx</DependentUpon>
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
		</Compile>
		<Compile Update="src\gui\data_grid_view_db_table.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>data_grid_view_db_table.resx</DependentUpon>
		</Compile>
		<Compile Update="src\gui\help_ui.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>help_ui.resx</DependentUpon>
		</Compile>
		<Compile Update="src\view3d\view3d_measurement_ui.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>view3d_measurement_ui.resx</DependentUpon>
		</Compile>
	</ItemGroup>
	<ItemGroup>
		<EmbeddedResource Update="Properties\Resources.resx">
			<CustomToolNamespace>Rylogic</CustomToolNamespace>
			<LastGenOutput>Resources.Designer.cs</LastGenOutput>
			<Generator>PublicResXFileCodeGenerator</Generator>
		</EmbeddedResource>
		<EmbeddedResource Update="src\gui\data_grid_view_db_table.resx">
			<Generator>PublicResXFileCodeGenerator</Generator>
			<LastGenOutput>data_grid_view_db_table.Designer.cs</LastGenOutput>
		</EmbeddedResource>
		<EmbeddedResource Update="src\gui\help_ui.resx">
			<Generator>PublicResXFileCodeGenerator</Generator>
			<LastGenOutput>help_ui.Designer.cs</LastGenOutput>
		</EmbeddedResource>
		<EmbeddedResource Update="src\view3d\view3d_measurement_ui.resx">
			<Generator>PublicResXFileCodeGenerator</Generator>
			<LastGenOutput>view3d_measurement_ui.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>
	<Target Name="PostBuild" AfterTargets="PostBuildEvent">
		<Exec Command="py $(ProjectDir)post_build.py $(ProjectDir) $(TargetDir) $(PlatformName) $(ConfigurationName)&#xD;&#xA;" />
	</Target>
</Project>